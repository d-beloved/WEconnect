{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "An app that connects businesses to individuals who might need their offered services",
  "main": "server/server.js",
  "scripts": {
    "start": "babel-node server/server.js",
    "start:dev": "nodemon server/server.js --exec babel-node ",
    "lint": "eslint ./ || true",
    "db-migrate": "sequelize db:migrate",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "test": "set NODE_ENV=test&& nyc mocha tests/routes/*.test.js --exit --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-beloved/WEconnect.git"
  },
  "keywords": [
    "Node",
    "Express",
    "PostgreSQL",
    "React"
  ],
  "author": "D-beloved",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-beloved/WEconnect/issues"
  },
  "homepage": "https://github.com/d-beloved/WEconnect#readme",
  "dependencies": {
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "email-validator": "^1.1.1",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.2.0",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.0",
    "sequelize-cli": "^4.0.0",
    "swagger-ui-express": "^2.0.15",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.1",
    "nyc": "^11.6.0",
    "supertest": "^3.0.0"
  }
}
